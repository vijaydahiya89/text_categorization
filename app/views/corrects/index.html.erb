<h1>Listing corrects</h1>

<table>
  <tr>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @corrects.each do |correct| %>
  <tr>
    <td><%= correct.status %></td>
    <td><%= link_to 'Show', correct %></td>
    <td><%= link_to 'Edit', edit_correct_path(correct) %></td>
    <td><%= link_to 'Destroy', correct, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Correct', new_correct_path %>
