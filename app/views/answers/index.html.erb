<h1>Listing answers</h1>

<table>
  <tr>
    <th>Answer</th>
    <th>Question</th>
    <th>Status</th>
  </tr>

<% for answer in @answers %>
  <tr>
    <td><%=h answer.answer %></td>
    <td><%=h answer.question_id %></td>
    <td><%=h answer.status %></td>
  </tr>
<% end %>
</table>
